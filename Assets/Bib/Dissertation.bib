@article{seowTransferEffectsVaried2019,
  title = {Transfer Effects of Varied Practice and Adaptation to Changes in Complex Skill Acquisition},
  author = {Seow, Roderick Yang Terng and Betts, Shawn and Anderson, John R},
  year = {2019},
  journal = {Proceedings of the 17th International Conference on Cognitive Modelling},
