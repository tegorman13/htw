@article{gouetBiasNoiseProportion2021,
  title = {Bias and Noise in Proportion Estimation: {{A}} Mixture Psychophysical Model},
  shorttitle = {Bias and Noise in Proportion Estimation},
  author = {Gouet, Camilo and Jin, Wei and Naiman, Daniel Q. and Pe{\~n}a, Marcela and Halberda, Justin},
  year = {2021},
  month = aug,
  journal = {Cognition},
  series = {Special {{Issue}} in {{Honour}} of {{Jacques Mehler}}, {{Cognition}}'s Founding Editor},
