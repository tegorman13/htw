@article{thompsonVideoGameTelemetry2013,
  title = {Video {{Game Telemetry}} as a {{Critical Tool}} in the {{Study}} of {{Complex Skill Learning}}},
  author = {Thompson, Joseph J. and Blair, Mark R. and Chen, Lihan and Henrey, Andrew J.},
  year = {2013},
